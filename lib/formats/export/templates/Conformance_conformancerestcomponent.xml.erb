<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.mode().nil? -%><mode value="<%= model.mode() %>"/><%- end -%>
    <%- if !model.documentation().nil? -%><documentation value="<%= model.documentation() %>"/><%- end -%>
    <%- if !model.security().nil? -%><%== render :template => 'Conformance_conformancerestsecuritycomponent', :locals => {name: 'security', model: model.security()} %><%- end -%>
    <%- if (!model.resource().nil? && !model.resource.empty?) -%><%- model.resource().each do |element| -%><%== render :template => 'Conformance_conformancerestresourcecomponent', :locals => {name: 'resource', model: element} %><%- end -%><%- end -%>
    <%- if (!model.operation().nil? && !model.operation.empty?) -%><%- model.operation().each do |element| -%><%== render :template => 'Conformance_conformancerestoperationcomponent', :locals => {name: 'operation', model: element} %><%- end -%><%- end -%>
    <%- if (!model.query().nil? && !model.query.empty?) -%><%- model.query().each do |element| -%><%== render :template => 'Conformance_conformancerestquerycomponent', :locals => {name: 'query', model: element} %><%- end -%><%- end -%>
    <%- if (model.documentMailbox() && !model.documentMailbox.empty?) -%><%- model.documentMailbox().each do |element| -%><documentMailbox value="<%= element %>"/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
