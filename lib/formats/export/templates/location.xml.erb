<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Location' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if !model.identifier().nil? -%><%== render :template => 'identifier', :locals => {name: 'identifier', model: model.identifier()} %><%- end -%>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description() %>"/><%- end -%>
    <%- if !model.fhirType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'type', model: model.fhirType()} %><%- end -%>
    <%- if (!model.telecom().nil? && !model.telecom.empty?) -%><%- model.telecom().each do |element| -%><%== render :template => 'contact', :locals => {name: 'telecom', model: element} %><%- end -%><%- end -%>
    <%- if !model.address().nil? -%><%== render :template => 'address', :locals => {name: 'address', model: model.address()} %><%- end -%>
    <%- if !model.physicalType().nil? -%><%== render :template => 'codeableconcept', :locals => {name: 'physicalType', model: model.physicalType()} %><%- end -%>
    <%- if !model.position().nil? -%><%== render :template => 'location_locationpositioncomponent', :locals => {name: 'position', model: model.position()} %><%- end -%>
    <%- if !model.managingOrganization().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'managingOrganization', model: model.managingOrganization()} %><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.partOf().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'partOf', model: model.partOf()} %><%- end -%>
    <%- if !model.mode().nil? -%><mode value="<%= model.mode() %>"/><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
