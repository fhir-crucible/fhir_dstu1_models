<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.software().nil? -%><software value="<%= model.software() %>"/><%- end -%>
    <%- if !model.versionNum().nil? -%><version value="<%= model.versionNum() %>"/><%- end -%>
    <%- if !model.contact().nil? -%><%== render :template => 'contact', :locals => {name: 'contact', model: model.contact()} %><%- end -%>
    <%- if !model.endpoint().nil? -%><endpoint value="<%= model.endpoint() %>"/><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
