<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'AllergyIntolerance' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model} %>
    <%== render :template => 'resource', :locals => {model: model} %>
    <%- if (!model.identifier().nil? && !model.identifier.empty?) -%><%- model.identifier().each do |element| -%><%== render :template => 'Identifier', :locals => {name: 'identifier', model: element} %><%- end -%><%- end -%>
    <%- if !model.criticality().nil? -%><criticality value="<%= model.criticality() %>"/><%- end -%>
    <%- if !model.sensitivityType().nil? -%><sensitivityType value="<%= model.sensitivityType() %>"/><%- end -%>
    <%- if !model.recordedDate().nil? -%><recordedDate value="<%= model.recordedDate().iso8601 %>"/><%- end -%>
    <%- if !model.status().nil? -%><status value="<%= model.status() %>"/><%- end -%>
    <%- if !model.subject().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'subject', model: model.subject()} %><%- end -%>
    <%- if !model.recorder().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'recorder', model: model.recorder()} %><%- end -%>
    <%- if !model.substance().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'substance', model: model.substance()} %><%- end -%>
    <%- if (!model.reaction().nil? && !model.reaction.empty?) -%><%- model.reaction().each do |element| -%><%== render :template => 'resourcereference', :locals => {name: 'reaction', model: element} %><%- end -%><%- end -%>
    <%- if (!model.sensitivityTest().nil? && !model.sensitivityTest.empty?) -%><%- model.sensitivityTest().each do |element| -%><%== render :template => 'resourcereference', :locals => {name: 'sensitivityTest', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
