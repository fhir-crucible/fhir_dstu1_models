<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.isExtensible().nil? -%><isExtensible value="<%= model.isExtensible() %>"/><%- end -%>
    <%- if !model.conformance().nil? -%><conformance value="<%= model.conformance() %>"/><%- end -%>
    <%- if !model.description().nil? -%><description value="<%= model.description() %>"/><%- end -%>
    <%- if !model.referenceUri().nil? -%><referenceUri value="<%= model.referenceUri() %>"/><%- end -%>
    <%- if !model.referenceResource().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'referenceResource', model: model.referenceResource()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
