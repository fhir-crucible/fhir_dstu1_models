<%== '<?xml version="1.0" encoding="UTF-8"?>' if is_root %>
<% local_name = name || 'Schedule' %>
<<%= (is_lowercase) ? local_name.downcase : local_name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
<%== (is_root) ? ' xmlns="http://hl7.org/fhir"' : ''%>>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if (!model.event().nil? && !model.event.empty?) -%><%- model.event().each do |element| -%><%== render :template => 'Period', :locals => {name: 'event', model: element} %><%- end -%><%- end -%>
    <%- if !model.repeat().nil? -%><%== render :template => 'Schedule_schedulerepeatcomponent', :locals => {name: 'repeat', model: model.repeat()} %><%- end -%>
</<%= (is_lowercase) ? local_name.downcase : local_name %>>
