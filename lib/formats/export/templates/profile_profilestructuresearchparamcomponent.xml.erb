<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.name().nil? -%><name value="<%= model.name() %>"/><%- end -%>
    <%- if !model.fhirType().nil? -%><type value="<%= model.fhirType() %>"/><%- end -%>
    <%- if !model.documentation().nil? -%><documentation value="<%= model.documentation() %>"/><%- end -%>
    <%- if !model.xpath().nil? -%><xpath value="<%= model.xpath() %>"/><%- end -%>
    <%- if (model.target() && !model.target.empty?) -%><%- model.target().each do |element| -%><target value="<%= element %>"/><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
