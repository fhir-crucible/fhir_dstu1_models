<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.species().nil? -%><%== render :template => 'CodeableConcept', :locals => {name: 'species', model: model.species()} %><%- end -%>
    <%- if !model.breed().nil? -%><%== render :template => 'CodeableConcept', :locals => {name: 'breed', model: model.breed()} %><%- end -%>
    <%- if !model.genderStatus().nil? -%><%== render :template => 'CodeableConcept', :locals => {name: 'genderStatus', model: model.genderStatus()} %><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
