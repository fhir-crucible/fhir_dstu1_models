<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.system().nil? -%><system value="<%= model.system() %>"/><%- end -%>
    <%- if !model.code().nil? -%><code value="<%= model.code() %>"/><%- end -%>
    <%- if !model.equivalence().nil? -%><equivalence value="<%= model.equivalence() %>"/><%- end -%>
    <%- if !model.comments().nil? -%><comments value="<%= model.comments() %>"/><%- end -%>
    <%- if (!model.product().nil? && !model.product.empty?) -%><%- model.product().each do |element| -%><%== render :template => 'conceptmap_otherconceptcomponent', :locals => {name: 'product', model: element} %><%- end -%><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
