<<%= (is_lowercase) ? name.downcase : name %>
<%if model.xmlId%> id="<%= model.xmlId%>"<%end%>
>
    <%== render :template => 'element', :locals => {model: model} %>
    <%- if !model.code().nil? -%><%== render :template => 'coding', :locals => {name: 'code', model: model.code()} %><%- end -%>
    <%- if !model.category().nil? -%><category value="<%= model.category() %>"/><%- end -%>
    <%- if !model.mode().nil? -%><mode value="<%= model.mode() %>"/><%- end -%>
    <%- if (!model.protocol().nil? && !model.protocol.empty?) -%><%- model.protocol().each do |element| -%><%== render :template => 'coding', :locals => {name: 'protocol', model: element} %><%- end -%><%- end -%>
    <%- if !model.focus().nil? -%><focus value="<%= model.focus() %>"/><%- end -%>
    <%- if !model.request().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'request', model: model.request()} %><%- end -%>
    <%- if !model.response().nil? -%><%== render :template => 'resourcereference', :locals => {name: 'response', model: model.response()} %><%- end -%>
    <%- if !model.documentation().nil? -%><documentation value="<%= model.documentation() %>"/><%- end -%>
</<%= (is_lowercase) ? name.downcase : name %>>
